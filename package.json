{
  "name": "static-docs-site",
  "version": "1.0.0",
  "description": "A modern, responsive static documentation site.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:gh-pages": "set NODE_ENV=production && npm run build",
    "preview": "vite preview",
    "update-manifest": "node scripts/update-manifest.js",
    "deploy": "npm run build:gh-pages && gh-pages -d dist"
  },
  "dependencies": {
