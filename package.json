    "@tabler/icons-webfont": "^3.34.1",
    "@tailwindcss/typography": "^0.5.16",
    "animate.css": "^4.1.1",
    "buffer": "^6.0.3",
    "chart.js": "^4.4.0",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.10",
    "lucide-react": "^0.294.0",
    "lunr": "^2.3.9",
    "mermaid": "^10.6.0",
    "prismjs": "^1.29.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "rehype-highlight": "^7.0.0",
    "rehype-katex": "^7.0.1",
